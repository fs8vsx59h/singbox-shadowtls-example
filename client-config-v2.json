{
  "outbounds": [
    {
      "type": "shadowsocks",
      "method": "2022-blake3-aes-128-gcm",
      "password": "base64",
      "detour": "shadowtls-out",
      "multiplex": {
        "enabled": true,
        "max_connections": 4,
        "min_streams": 4
      }
    },
    {
      "type": "shadowtls",
      "tag": "shadowtls-out",
      "server": "0.0.0.0",
      "server_port": 443,
      "version": 2,
      "tls": {
        "enabled": true,
        "server_name": "microsoft.com"
      }
    }
  ],
  "inbounds": [
    {
      "type": "socks",
      "tag": "socks-in",
      "listen": "127.0.0.1",
      "listen_port": 1080,
      "users": [
        {
          "username": "admin",
          "password": "admin"
        }
      ]
    }
  ]
}
